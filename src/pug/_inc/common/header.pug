
noscript
  iframe(src="https://www.googletagmanager.com/ns.html?id=GTM-NTN66FN", height="0", width="0", style="display:none;visibility:hidden")

header.header
  .header-row
    a.header-site-title(href="#concept") #{pageTitle}
    nav.header-gnav
      ul.header-gnav__list
        - for (var i = 0; i < gnav.length; i++)
          li.header-gnav__item
            a(class='header-gnav__link '+gnav[i]['icon'], href=gnav[i]['url'])
              | #{gnav[i]['title']}
      ul.header-sns
        - for (var i = 0; i < contact.length; i++)
          li
            a(class='header-sns__link '+contact[i]['icon'], href=contact[i]['url'], target="_blank", rel="noopener")
              i(class='icon-'+contact[i]['icon'])
